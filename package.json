{
	"name": "goblin",
	"version": "1.4.0",
	"private": true,
	"author": "r-priyam",
	"type": "module",
	"main": "dist/goblin.js",
	"imports": {
		"#root/*": "./dist/*.js",
		"#lib/*": "./dist/lib/*.js",
		"#lib/coc": "./dist/lib/coc/index.js",
		"#utils/*": "./dist/lib/util/*.js"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch --onSuccess \"yarn start\"",
		"format": "prettier --write --loglevel=warn \"src/**/*.ts\"",
		"lint": "eslint src --ext ts --fix",
		"release": "cliff-jumper",
		"release:test": "cliff-jumper --dry-run",
		"start": "node --enable-source-maps dist/goblin.js",
		"typecheck": "tsc -b src",
		"watch": "tsup --watch"
	},
	"resolutions": {
		"minimist": "^1.2.6"
	},
	"dependencies": {
		"@discordjs/builders": "^0.16.0",
		"@mikro-orm/sql-highlighter": "^1.0.1",
		"@sapphire/decorators": "^4.3.8",
		"@sapphire/discord.js-utilities": "^4.11.3",
		"@sapphire/fetch": "^2.4.1",
		"@sapphire/framework": "next",
		"@sapphire/plugin-logger": "^2.2.3",
		"@sapphire/plugin-scheduled-tasks": "^2.3.5",
		"@sapphire/stopwatch": "^1.4.1",
		"@sapphire/time-utilities": "^1.7.5",
		"@sapphire/type": "^2.2.4",
		"@sapphire/utilities": "^3.7.0",
		"@skyra/env-utilities": "^1.0.2",
		"@types/gradient-string": "^1.1.2",
		"bull": "^4.8.4",
		"clashofclans.js": "2.8.0",
		"colorette": "^2.0.19",
		"discord-api-types": "^0.33.5",
		"discord.js": "^13.9.0",
		"figlet": "^1.5.2",
		"fuse.js": "^6.6.2",
		"gradient-string": "^2.0.1",
		"postgres": "^3.2.4",
		"redis": "^4.2.0",
		"reflect-metadata": "^0.1.13",
		"string-strip-html": "^11.0.0"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^1.8.5",
		"@sapphire/eslint-config": "^4.3.7",
		"@sapphire/prettier-config": "^1.4.3",
		"@sapphire/ts-config": "^3.3.4",
		"@types/bull": "^3.15.8",
		"@types/figlet": "^1.5.4",
		"@types/node": "^18.0.4",
		"@typescript-eslint/eslint-plugin": "^5.30.7",
		"@typescript-eslint/parser": "^5.30.7",
		"eslint": "^8.20.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-unicorn": "^43.0.2",
		"prettier": "^2.7.1",
		"tsup": "^6.1.3",
		"typescript": "^4.7.4"
	},
	"packageManager": "yarn@3.2.1"
}
