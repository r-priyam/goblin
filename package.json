{
	"name": "goblin",
	"version": "1.7.0",
	"private": true,
	"author": "r-priyam",
	"type": "module",
	"imports": {
		"#root/*": "./dist/*.js",
		"#lib/*": "./dist/lib/*.js",
		"#lib/coc": "./dist/lib/coc/index.js",
		"#utils/*": "./dist/lib/util/*.js"
	},
	"main": "dist/goblin.js",
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch --onSuccess \"yarn start\"",
		"format": "prettier --write . && eslint src --ext ts --fix",
		"lint": "prettier --check . && eslint src --ext ts",
		"release": "cliff-jumper",
		"release:test": "cliff-jumper --dry-run",
		"start": "node --enable-source-maps dist/goblin.js",
		"sync-docker": "node scripts/syncDocker.mjs",
		"typecheck": "tsc -b src",
		"watch": "tsup --watch"
	},
	"resolutions": {
		"minimist": "^1.2.6"
	},
	"dependencies": {
		"@discordjs/builders": "^0.16.0",
		"@discordjs/rest": "^1.1.0",
		"@sapphire/cron": "^1.0.0",
		"@sapphire/decorators": "^5.0.0",
		"@sapphire/discord.js-utilities": "^5.0.1",
		"@sapphire/framework": "3.1.1",
		"@sapphire/plugin-logger": "3.0.1",
		"@sapphire/plugin-scheduled-tasks": "4.0.1",
		"@sapphire/plugin-subcommands": "3.2.1",
		"@sapphire/stopwatch": "^1.4.1",
		"@sapphire/type": "^2.2.4",
		"@sapphire/utilities": "^3.9.3",
		"@skyra/env-utilities": "^1.0.2",
		"bullmq": "^2.1.0",
		"clashofclans.js": "2.8.4",
		"colorette": "^2.0.19",
		"discord-api-types": "^0.33.5",
		"discord.js": "^13.11.0",
		"figlet": "^1.5.2",
		"fuse.js": "^6.6.2",
		"gradient-string": "^2.0.2",
		"markdown-table": "^3.0.2",
		"postgres": "^3.2.4",
		"redis": "^4.3.1",
		"reflect-metadata": "^0.1.13",
		"string-strip-html": "^11.6.4"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^1.8.7",
		"@sapphire/prettier-config": "^1.4.4",
		"@sapphire/ts-config": "^3.3.4",
		"@swc/core": "^1.3.1",
		"@types/figlet": "^1.5.5",
		"@types/gradient-string": "^1.1.2",
		"@types/node": "^18.7.18",
		"@typescript-eslint/eslint-plugin": "^5.37.0",
		"@typescript-eslint/parser": "^5.37.0",
		"eslint": "^8.23.1",
		"eslint-config-neon": "^0.1.33",
		"prettier": "^2.7.1",
		"tsup": "^6.2.3",
		"typescript": "^4.8.3"
	},
	"packageManager": "yarn@3.2.3"
}
