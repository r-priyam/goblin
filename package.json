{
	"name": "goblin",
	"version": "1.0.0",
	"private": true,
	"author": "r-priyam",
	"type": "module",
	"main": "dist/goblin.js",
	"imports": {
		"#root/*": "./dist/*.js",
		"#lib/*": "./dist/lib/*.js",
		"#utils/*": "./dist/lib/util/*.js"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch --onSuccess \"yarn start\"",
		"format": "prettier --write --loglevel=warn \"src/**/*.ts\"",
		"lint": "eslint src --ext ts --fix",
		"start": "node --enable-source-maps dist/goblin.js",
		"typecheck": "tsc -b src",
		"watch": "tsup --watch"
	},
	"prettier": "@sapphire/prettier-config",
	"resolutions": {
		"minimist": "^1.2.6"
	},
	"dependencies": {
		"@sapphire/decorators": "^4.3.4",
		"@sapphire/discord-utilities": "^2.10.3",
		"@sapphire/discord.js-utilities": "^4.10.0",
		"@sapphire/fetch": "^2.3.1",
		"@sapphire/framework": "next",
		"@sapphire/plugin-logger": "^2.2.1",
		"@sapphire/time-utilities": "^1.7.4",
		"@sapphire/type": "^2.2.4",
		"@sapphire/utilities": "^3.6.2",
		"@types/gradient-string": "^1.1.2",
		"colorette": "^2.0.16",
		"discord-api-types": "^0.30.0",
		"discord.js": "^13.7.0",
		"dotenv": "^16.0.1",
		"figlet": "^1.5.2",
		"gradient-string": "^2.0.1",
		"reflect-metadata": "^0.1.13"
	},
	"devDependencies": {
		"@sapphire/eslint-config": "^4.3.4",
		"@sapphire/prettier-config": "^1.4.3",
		"@sapphire/ts-config": "^3.3.4",
		"@types/eslint": "^8",
		"@types/figlet": "^1.5.4",
		"@types/node": "^17.0.33",
		"eslint": "^8.15.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "^2.6.2",
		"tsup": "^5.12.7",
		"typescript": "^4.6.4"
	},
	"packageManager": "yarn@3.2.0"
}
